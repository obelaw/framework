{
    "name": "obelaw/obelaw",
    "description": "This is Obelaw's framework for building modules",
    "autoload": {
        "psr-4": {
            "Obelaw\\Framework\\": "src/",
            "Obelaw\\UI\\": "packages/ui/src/",
            "Obelaw\\Permissions\\": "addons/permissions/src/"
        },
        "files": [
            "helpers/autoload.php",
            "addons/permissions/helpers/autoload.php"
        ]
    },
    "authors": [
        {
            "name": "Karim Mohamed",
            "email": "komicho1996@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^10.0||^11.0",
        "livewire/livewire": "^3.3",
        "jantinnerezo/livewire-alert": "^3.0",
        "maatwebsite/excel": "^3.1",
        "obelaw/o-engine": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obelaw\\Framework\\ObelawServiceProvider",
                "Obelaw\\UI\\ObelawUIServiceProvider",
                "Obelaw\\Permissions\\ObelawPermissionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}