{
    "name": "obelaw/framework",
    "description": "This is Obelaw's framework for building modules",
    "autoload": {
        "psr-4": {
            "Obelaw\\Framework\\": "src/"
        },
        "files": [
            "helpers/autoload.php"
        ]
    },
    "authors": [
        {
            "name": "Karim Mohamed",
            "email": "komicho1996@gmail.com"
        }
    ],
    "require": {
        "obelaw/ui": "^1.0@beta",
        "obelaw/permissions": "dev-main",
        "obelaw/o-engine": "^1.0@beta"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obelaw\\Framework\\ObelawServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
